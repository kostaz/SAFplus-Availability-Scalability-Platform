<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<openClovisAsp>
  <version v0="4.0.0">
    <multixport:multixport xmlns:multixport="multixport.ecore">
      <xports>
        <xport>
          <type>TIPC</type>
          <plugin>libClTIPC.so</plugin>
        </xport>
        <xport>
          <type>UDP</type>
          <plugin>libClUDP.so</plugin>
        </xport>
      </xports>
      <protocol default="TIPC">
        <node name="SCNodeI0" protocol="TIPC UDP"/>
        <node name="SCNodeI1" protocol="TIPC UDP"/>
        <node name="PayloadNodeI0" protocol="TIPC UDP"/>
        <node name="PayloadNodeI1" protocol="TIPC UDP"/>
      </protocol>
      <heartbeat>
          <interval>1000</interval>
          <intervalLocal>1000</intervalLocal>
          <retries>3</retries>
      </heartbeat>
    </multixport:multixport>
  </version>
</openClovisAsp>
